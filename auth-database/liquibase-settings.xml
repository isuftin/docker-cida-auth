<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 https://maven.apache.org/xsd/settings-1.0.0.xsd">
    <profiles>
        <profile>
            <id>cida-auth-liquibase-postgres</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <db.auth.schemaname>
                    public
                </db.auth.schemaname>
                <db.auth.dbname>
                    cida_auth
                </db.auth.dbname>
                <db.auth.user>
                    postgres
                </db.auth.user>
                <db.auth.pass>
                    postgres
                </db.auth.pass>
                <db.auth.runasusername>
                    postgres
                </db.auth.runasusername>
                <db.auth.host>
                    127.0.0.1
                </db.auth.host>
                <db.auth.port>
                    5432
                </db.auth.port>
            </properties>
        </profile>
    </profiles>
</settings>
